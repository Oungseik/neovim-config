    ]
  },

  "svelte select with ShadCN and @tanstack/svelte-form": {
    "prefix": "sv-select",
    "body": [
      "<form.Field name=\"$1\">",
      "  {#snippet children(field)}",
      "    <div class=\"space-y-2\">",
      "      <Label for={field.name}>$2</Label>",
      "      <Select.Root",
      "        type=\"$3\"",
      "        name={field.name}",
      "        value={field.state.value}",
      "        onValueChange={(value) => {",
      "          field.handleChange($4);",
      "        }}",
      "      >",
      "        <Select.Trigger class=\"w-full\">$5</Select.Trigger>",
      "        <Select.Content>",
      "          <Select.Group>",
      "            <Select.Label>$6</Select.Label>",
      "              <Select.Item value={$7}>{$8}</Select.Item>",
      "          </Select.Group>",
      "        </Select.Content>",
      "      </Select.Root>",
      "    </div>",
      "  {/snippet}",
      "</form.Field>"
    ]
  }
}
