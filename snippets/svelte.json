      "<form.Field name=\"$1\">",
      " {#snippet children(field)}",
      "   <div class=\"space-y-2\">",
      "     <Label for={field.name}>$2</Label>",
      "     <Input",
      "       id={field.name}",
      "       name={field.name}",
      "       value={field.state.value}",
      "       type=\"$3\"",
      "       onblur={field.handleBlur}",
      "       onchange={(e) => field.handleChange(e.currentTarget.value)}",
